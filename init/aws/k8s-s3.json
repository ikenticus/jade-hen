{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                        "s3:GetBucketLocation",
                        "s3:GetObject",
                        "s3:ListAllMyBuckets",
                        "s3:PutObject"
                      ],
            "Resource": "arn:aws:s3:::*"
        },
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::jade-hen",
                "arn:aws:s3:::jade-hen/*"
            ]
        }
    ]
}

